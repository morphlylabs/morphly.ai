{
  "$schema": "https://turborepo.org/schema.json",
  "extends": ["//"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env"],
      "outputs": [".next/**", "!.next/cache/**", "next-env.d.ts"],
      "env": [
        "TURSO_DATABASE_URL",
        "TURSO_AUTH_TOKEN",
        "BETTER_AUTH_SECRET",
        "REDIS_URL",
        "AI_GATEWAY_API_KEY",
        "NODE_ENV",
        "AWS_REGION",
        "AWS_ACCESS_KEY_ID",
        "AWS_SECRET_ACCESS_KEY",
        "AWS_LAMBDA_CQTOSTL_FUNCTION_NAME",
        "GOOGLE_CLIENT_ID",
        "GOOGLE_CLIENT_SECRET",
        "AUTUMN_SECRET_KEY"
      ]
    },
    "dev": {
      "persistent": true
    }
  }
}
